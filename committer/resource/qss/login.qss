/* 主窗体样式 */
#Login {
    background-color: #212121;
}

/* 行编辑器样式 */
QLineEdit {
    border-style: none;
    background-color: #3e3e3e;
    color: #eeeeee;
    border-radius: 5px;
}

/* 按钮样式 */
QPushButton {
    border-style: solid;
    border-width: 2px;
    border-color: #444444;
    border-radius: 5px;
    background-color: #333333;
    color: #eeeeee;
}

/* 按钮样式 悬停 */
QPushButton:hover {
    border-style: solid;
    border-width: 2px;
    border-color: #444444;
    border-radius: 5px;
    background-color: #3e3e3e;
    color: #eeeeee;
}

/* 按钮样式 按下 */
QPushButton:pressed {
    border-style: solid;
    border-width: 2px;
    border-color: #444444;
    border-radius: 5px;
    background-color: #333333;
    color: #eeeeee;
}